  <body vocab="https://schema.org" typeof="Festival">
    <link property="eventStatus" href="https://schema.org/EventScheduled" />
    <link
      property="eventAttendanceMode"
      href="https://schema.org/MixedEventAttendanceMode"
    />
    <!-- Google Tag Manager (noscript) -->
    <noscript>
      <iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-5F59KHT"
        height="0"
        width="0"
        style="display: none; visibility: hidden"
      ></iframe>
    </noscript>
