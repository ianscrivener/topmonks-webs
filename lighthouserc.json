{
  "ci": {
    "collect": {
      "url": "https://localhost:8000/",
      "settings": {
        "skipAudits": [
          "themed-omnibox",
          "redirects-http",
          "load-fast-enough-for-pwa",
          "service-worker",
          "splash-screen",
          "webapp-install-banner",
          "works-offline"
        ]
      }
    },
    "assert": {
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "lhci",
      "serverBaseUrl": "https://lighthouse.dev.tmcloud.io/"
    }
  }
}
