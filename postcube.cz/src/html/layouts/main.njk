<!doctype html>
<html lang="cs" prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# business: http://ogp.me/ns/business#">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta http-equiv="Accept-CH" content="DPR,Viewport-Width,Width,Save-Data">
  <meta name="viewport" content="width=device-width,initial-scale=1,viewport-fit=cover">
  <meta name="format-detection" content="telephone=no">
  <title>{% block title %}{{ language[page].title }}{% endblock %}</title>
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link rel="preconnect" href="https://res.cloudinary.com">
  {% block preconnect %}{% endblock %}
  {% block preload %}{% endblock %}

  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
  <style>
    @font-face {
      font-family: "Roobert";
      src: local("Roobert Regular"),
      url("/assets/css/Roobert-Regular.woff2") format("woff2"),
      url("/assets/css/Roobert-Regular.woff") format("woff"),
      url("/assets/css/Roobert-Regular.ttf") format("tff");
      font-weight: 400;
      font-style: normal;
      font-display: swap;
    }

    @font-face {
      font-family: "Roobert";
      src: local("Roobert Bold"),
      url("/assets/css/Roobert-Bold.woff2") format("woff2"),
      url("/assets/css/Roobert-Bold.woff") format("woff"),
      url("/assets/css/Roobert-Bold.tff") format("tff");
      font-weight: 700;
      font-style: normal;
      font-display: swap;
    }
  </style>
  <link rel="stylesheet" href="/assets/css/typography.css">
  <link rel="stylesheet" href="/assets/css/main.css">
  <link rel="stylesheet" href="/assets/css/hp.css">

  {% block head %}{% endblock %}
</head>
<body id="page-{{page}}" vocab="https://schema.org/" typeof="{{ pageType or 'WebPage' }}">

<link rel="stylesheet" href="/assets/css/header.css">
{% include 'components/header.njk' %}

<main id="content">
  {% block content %}{% endblock %}
</main>

<link rel="stylesheet" href="/assets/css/footer.css">
{% include 'components/footer.njk' %}


{% block javascript %}{% endblock %}

</body>
</html>

